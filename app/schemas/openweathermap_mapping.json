{
    "data_type": "weather",
    "type": "webservice",
    "url": "https://api.openweathermap.org/data/2.5/weather?q={}&appid={}",
    "is_list": "false",
    "params": ["locationName", "appid"],
    "response_type": "json",
    "schema":{
        "provider" : {"type" : "string", "value" : "OpenWeatherMap API"},
        "datetime" : {"type" : "timestamp", "path": "dt"},
        "location" : {"type" : "latlon", 
                      "lat" : {"type" : "double", "unit" : "wsg84", "path" : "coord.lat"},
                      "lon" : {"type" : "double", "unit" : "wsg84", "path" : "coord.lon"}
                     },
        "lat" : {"type" : "double", "unit" : "wsg84", "path" : "coord.lat"},
        "lon" : {"type" : "double", "unit" : "wsg84", "path" : "coord.lon"},
        "main_temp" : {"type" : "double", "unit" : "degrees_Kelvin", "path": "main.temp"},
        "min_temp" : {"type" : "double", "unit" : "degrees_Kelvin", "path": "main.temp_min"},
        "max_temp" : {"type" : "double", "unit" : "degrees_Kelvin", "path": "main.temp_max"},
        "feelslike_temp" : {"type" : "double", "unit" : "degrees_Kelvin", "path": "main.feels_like"},
        "main_pressure" : {"type" : "double", "unit" : "hpa", "path": "main.pressure"},
        "humidity" : {"type" : "double", "unit" : "percentage", "path": "main.humidity"},
        "sea_level" : {"type" : "double", "unit" : "meters", "path": "main.sea_level"},
        "ground_level" : {"type" : "double", "unit" : "meters", "path": "main.grnd_level"},
        "main_visibility" : {"type" : "double", "unit" : "meters", "path": "visibility"},
        "main_wind_speed" : {"type" : "double", "unit" : "knots", "path": "wind.speed"},
        "main_wind_direction" : {"type" : "double", "unit" : "angle_degrees", "path": "wind.deg"},
        "main_wind_gust" : {"type" : "double", "unit" : "meter_second", "path": "wind.gust"},
        "main_clouds" : {"type" : "double", "unit" : "percentage", "path": "clouds.all"},
        "main_type" : {"type" : "double", "path": "sys.type"},
        "system_id" : {"type" : "double", "path": "sys.id"},
        "country" : {"type" : "string", "path": "sys.country"},
        "sunrise_time" : {"type" : "timestamp", "path": "sys.sunrise"},
        "sunset_time" : {"type" : "timestamp", "path": "sys.sunset"},
        "timezone" : {"type" : "timestamp", "unit" : "unix" ,"path": "timezone"},
        "main_id" : {"type" : "double","path": "id"},
        "main_name" : {"type" : "string", "path": "name"},
        "cod" : {"type" : "double", "path": "cod"}
    }
}